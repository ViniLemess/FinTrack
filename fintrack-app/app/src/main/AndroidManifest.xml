<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/fintrack_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fintrack_icon"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Fintrack">
        <activity
            android:name="org.fundatec.vinilemess.tcc.fintrackapp.activities.TransactionRegistryActivity"
            android:exported="false" />
        <activity
            android:name="org.fundatec.vinilemess.tcc.fintrackapp.activities.HomeActivity"
            android:exported="false" />
        <activity
            android:name="org.fundatec.vinilemess.tcc.fintrackapp.activities.TransactionsActivity"
            android:exported="false" />
        <activity
            android:name="org.fundatec.vinilemess.tcc.fintrackapp.activities.UserRegistryActivity"
            android:exported="false" />
        <activity
            android:name="org.fundatec.vinilemess.tcc.fintrackapp.activities.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.fundatec.vinilemess.tcc.fintrackapp.activities.LoginActivity"
            android:exported="true" />
    </application>

</manifest>